import itertools

n = int(input())
data = list(map(int, input().split()))

result = []

# 조합구하기
for i in range(1, n+1):
    combi = list(itertools.combinations(data, i))
    for c in combi:
        sum = 0
        for j in range(i):
            sum += c[j]
        result.append(sum)

# result.sort()

for i in range(1, 1000001):
    if i in result:
        continue
    else:
        print(i)
        break
