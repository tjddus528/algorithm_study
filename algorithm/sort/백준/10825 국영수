import sys
input = sys.stdin.readline
n = int(input())
students = []
for _ in range(n):
    name, korean, english, math = input().split()
    students.append((name, int(korean), int(english), int(math)))

def quick_sort_desc(array, p):
    if len(array) <= 1:
        return array
    if p > 3:
        return array
    pivot = array[0][p]
    tail = array[1:]
    print(pivot)
    left = [x for x in tail if x[p] > pivot]
    right = [x for x in tail if x[p] < pivot]
    middle = [x for x in tail if x[p] == pivot]

    return quick_sort_desc(left, p) + quick_sort_asc(middle+[pivot], p+1) + quick_sort_desc(right, p)

def quick_sort_asc(array, p):
    if len(array) <= 1:
        return array
    if p > 3:
        return array
    pivot = array[0][p]
    tail = array[1:]
    print(pivot)
    left = [x for x in tail if x[p] < pivot]
    right = [x for x in tail if x[p] > pivot]
    middle = [x for x in tail if x[p] == pivot]

    return quick_sort_desc(left, p) + quick_sort_desc(middle+[pivot], p+1) + quick_sort_desc(right, p)

result = quick_sort_desc(students, 1)

print(result)
